# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Thread {
  id: String!
  title: String!
  description: String!
  closed: Boolean!
}

type Query {
  threads: [Thread!]!
}

input NewThread {
  title: String!
  description: String!
}

type Mutation {
  createThread(input: NewThread!): Thread!
}
